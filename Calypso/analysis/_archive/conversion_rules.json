{
  "heading_rules": {
    "h1": {
      "font": "Arial-BoldMT",
      "size": 18.317365050315857,
      "also_match": "h1_caps"
    },
    "h2": {
      "font": "Arial-BoldItalicMT",
      "size": 74.65475463867188,
      "also_match": "h2_numbered"
    },
    "h3": {
      "font": "TimesNewRomanPS-BoldItal",
      "size": 41.025518798828124,
      "also_match": "h3_dotted"
    }
  },
  "text_rules": {
    "paragraph": {
      "font": "TimesNewRomanPS-BoldMT"
    },
    "list": {
      "pattern": "^[-\u2022*]\\s+|^\\d+\\)\\s+"
    },
    "code": {
      "pattern": "^```|^\\s{4,}"
    }
  },
  "layout_rules": {
    "page_size": {
      "average_page_width": 612.0,
      "average_page_height": 792.0,
      "layout_elements": {}
    },
    "margins": {
      "default": 36
    }
  }
}